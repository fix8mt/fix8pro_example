<?xml version='1.0' encoding='ISO-8859-1'?>
<fix8>
	<default role="acceptor"
				fix_version="1100"
				heartbeat_interval="10"
				ip="0.0.0.0"
				session_log="session_log_file"
				protocol_log="protocol_log"
				tcp_recv_buffer="100663296"
				tcp_send_buffer="100663296"
				tcp_nodelay="true"
				reset_sequence_numbers="false"
				ignore_logon_sequence_check="true"
				permissive_mode="false"
				process_model="threaded"
				enforce_compids="false"
				silent_disconnect="false"
				persist="file0"/>

	<ha      name="ha_set_1"
				role="primary"
				ip="192.168.0.32/29"
				port="55445"
				group="TEST" />

	<supplementals name="Set1">
		<field number='9991' name='SampleUserField2' type='STRING' messages='NewOrderSingle:N ExecutionReport:N OrderCancelRequest:Y' />
		<field number='9999' name='SampleUserField'  type='STRING' messages='NewOrderSingle:N ExecutionReport:N OrderCancelRequest:Y' />
	</supplementals>

	<session name="TEX"
				port="11001"
				active="true"
				supplementals="Set1"
				permissive_mode="true"
				sender_comp_id="TEX_DLD" />

	<session name="TEX_NP"
				port="11001"
				active="true"
				supplementals="Set1"
				permissive_mode="true"
				persist=""
				sender_comp_id="TEX_DLD" />

	<session name="TEXCANON"
				port="11001"
				active="true"
				decoder="canonical"
				supplementals="Set1"
				permissive_mode="true"
				session_log="session_log_file1"
				sender_comp_id="TEX_DLD" />

	<session name="TEX1"
				port="11001"
				supplementals="Set1"
				active="true"
				sender_comp_id="TEX1_DLD" />

	<session name="TEX2"
				port="11002"
				active="true"
				permissive_mode="true"
				supplementals="Set1"
				sender_comp_id="TEX2_DLD2" />

	<session name="TEX3"
				port="11003"
				active="true"
				permissive_mode="true"
				supplementals="Set1"
				sender_comp_id="TEX3_DLD3" />

	<session name="TEX6"
				port="11006"
				active="true"
				permissive_mode="true"
				supplementals="Set1"
				sender_comp_id="TEX6_DLD6" />

	<session name="TEX7"
				port="11007"
				active="true"
				permissive_mode="true"
				supplementals="Set1"
				sender_comp_id="TEX7_DLD7" />

	<session name="TEX8"
				port="11008"
				active="true"
				permissive_mode="true"
				supplementals="Set1"
				sender_comp_id="TEX8_DLD8" />

	<schedule name="test_schedule0"
				start_time="22:00:00"
				end_time="22:01:00"
				start_day="0"
				end_day="1"
				reject_code="7"
				reject_text="Test system is not accepting any business messages at the moment"
				utc_offset_mins="!{ now=`date +%z`; echo $(( ($now / 100) * 60 + ($now % 100) )) }" />

	<login   name="test_login0"
				start_time="16:56:00"
				end_time="16:57:00"
				start_day="0"
				end_day="6"
				utc_offset_mins="!{ now=`date +%z`; echo $(( ($now / 100) * 60 + ($now % 100) )) }" />

	<persist name="file1"
				type="file" dir="./run"
				use_session_id="true"
				rotation="5"
				db="inbound_server" />

	<persist name="bdb0"
				type="bdb"
				dir="./run"
				db="myfix_server.db"/>

	<persist name="file0"
				type="file" dir="./run"
				use_session_id="true"
				db="server" />

	<persist name="memcached0"
				type="memcached"
				config_string="--SERVER=127.0.0.1"
				db="server" />

	<persist name="redis0"
				type="hiredis"
				host="127.0.0.1" port="6379" connect_timeout="1"
				db="server" />

	<persist name="mem0"
				type="memory"/>

	<log 		name="session_log"
				type="pipe"
				filename="|/bin/cat"
				rotation="5"
				flags="timestamp|sequence|thread|level"/>

	<log 		name="session_log_file1"
				type="file"
				filename="./run/myfix_server_session.log"
				rotation="5"
				levels="debug|info|warn|error|fatal"
				flags="mstart|sequence|timestamp|sstart|thread|location|level"/>

	<log 		name="session_log_file"
				type="file"
				filename="./run/myfix_server_session.log"
				rotation="5"
				nodelimiter="[]"
				levels="debug|info|warn|error|fatal"
				flags="mstart|sequence|timestamp|sstart|thread|location|level"
				start_time="15:21:00"
				noutc_offset_mins="!{ now=`date +%z`; echo $(( ($now / 100) * 60 + ($now % 100) )) }"
				utc_offset_mins="auto" />

	<log 		name="protocol_log"
				type="file"
				filename="./run/myfix_server_protocol.log"
				rotation="5"
				flags="sequence|append|direction|inbound|outbound"/>

	<log 		name="protocol_log_pipe"
				type="pipe"
				flags="sequence|append|direction|inbound|outbound"
				filename="|/bin/cat"/>

	<client_group name="client_Group0">
		<client name="Goldsteins" target_comp_id="DLD_TEX" ip="192.168.0.37" active="false"/>
		<client name="Goldsteins" target_comp_id="DLD_TEX" ip="127.0.0.1" />
	</client_group>

</fix8>
